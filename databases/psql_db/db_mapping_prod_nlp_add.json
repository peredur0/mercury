{
  "mail_features_prod": {
    "mot_corpus": {
      "id_mot": ["SERIAL", "PRIMARY KEY"],
      "mot": ["VARCHAR", "UNIQUE", "NOT NULL"],
      "freq_corpus": ["INT"],
      "freq_documents": ["INT"]
    },
    "mots_document": {
      "id_message": ["INT"],
      "id_mot": ["INT"],
      "occurrence": ["INT"],
      "pk": ["id_message", "id_mot"],
      "fk": {
        "fk_message": ["id_message", "messages(id_message)", "SET NULL"],
        "fk_mot": ["id_mot", "mot_corpus(id_mot)", "SET NULL"]
      }
    },
    "nlp_status": {
      "id_message": ["INT"],
      "success": ["BOOL"],
      "raison": ["VARCHAR"],
      "pk": ["id_message"],
      "fk": {
        "fk_message": ["id_message", "messages(id_message)", "SET NULL"]
      }
    }
  }
}